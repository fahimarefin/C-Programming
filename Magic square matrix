#include<stdio.h> 
int main() 
{ int a[50][50]; int i,j,n,m,rowsum,columnsum,diagonalsum;
printf("Enter The Order Of The Matrix= \n"); 
scanf("%d",&n); scanf("%d",&m); 
printf("Enter The Element Of The Matrix= \n"); 
for(i=0; i<n; i++)
{ for(j=0; j<m; j++) 
{ scanf("%d",&a[i][j]); } } 
printf("Matrix is = \n"); 
for(i=0; i<n; i++) 
{ for(j=0; j<m; j++) 
{ printf("%d ",a[i][j]); }
printf("\n"); }

for(i=0; i<n; i++)
{
    rowsum=0;
    for(j=0; j<m; j++)
    {
        rowsum=rowsum+a[i][j]; //row wise sum;
    }
}
printf("rowsum= %d\n",rowsum);
for(i=0; i<n; i++)
{
    columnsum=0;
    for(j=0; j<m; j++)
    {
        columnsum=columnsum+a[j][i]; //column wise sum;
    }
}
printf("columnsum= %d\n",columnsum);
diagonalsum=0;
for(i=0; i<n; i++)
{
    for(j=0; j<m; j++)
    {
        if(i==j)
        {
            diagonalsum=diagonalsum+a[i][j];
        }
    }
}
printf("Diagonalsum= %d\n",diagonalsum);
if(rowsum==diagonalsum && columnsum==diagonalsum)
{
    printf("Given Matrix is A square Matrix");
}
else
{
    printf("Given Matrix Is Not A Square Matrix");
}
return 0;
}
